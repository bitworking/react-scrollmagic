{
  "name": "react-scrollmagic",
  "version": "2.3.0",
  "description": "React declarative component for ScrollMagic",
  "author": "bitworking",
  "license": "MIT",
  "repository": "bitworking/react-scrollmagic",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "copy": "copyfiles -f src/index.d.ts dist",
    "prepare": "yarn run build && npm run copy",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0-0 || ^17.0.0-0",
    "react-dom": "^16.0.0-0 || ^17.0.0-0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "9.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.1.4",
    "eslint": "5.6.0",
    "eslint-config-react-app": "3.0.3",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "2.6.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-standard": "^3.1.0",
    "gh-pages": "2.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "3.0.0",
    "rollup": "0.68.2",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-cpy": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-url": "^1.4.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {}
}
